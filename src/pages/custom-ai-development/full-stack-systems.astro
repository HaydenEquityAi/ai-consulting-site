  <script>
    // FAQ Accordion Functionality
    document.addEventListener('DOMContentLoaded', function() {
      const faqCards = document.querySelectorAll('.faq-card');
      
      faqCards.forEach(card => {
        card.addEventListener('click', function() {
          const isActive = this.classList.contains('active');
          
          // Close all FAQ cards
          faqCards.forEach(c => {
            c.classList.remove('active');
            const answer = c.querySelector('.faq-answer');
            const chevron = c.querySelector('.faq-chevron');
            if (answer) answer.classList.add('hidden');
            if (chevron) chevron.style.transform = 'rotate(0deg)';
          });
          
          // Open clicked card if it wasn't active
          if (!isActive) {
            this.classList.add('active');
            const answer = this.querySelector('.faq-answer');
            const chevron = this.querySelector('.faq-chevron');
            if (answer) answer.classList.remove('hidden');
            if (chevron) chevron.style.transform = 'rotate(180deg)';
          }
        });
      });

      // Industry Accordion Functionality
      const industryAccordions = document.querySelectorAll('.industry-accordion-item');
      
      industryAccordions.forEach(item => {
        const header = item.querySelector('.industry-accordion-header');
        const content = item.querySelector('.industry-accordion-content');
        const chevron = item.querySelector('.industry-accordion-chevron');
        
        header.addEventListener('click', () => {
          const isExpanded = header.getAttribute('aria-expanded') === 'true';
          
          // Close all other accordions (optional - remove if you want multiple open)
          industryAccordions.forEach(otherItem => {
            if (otherItem !== item) {
              const otherHeader = otherItem.querySelector('.industry-accordion-header');
              const otherContent = otherItem.querySelector('.industry-accordion-content');
              const otherChevron = otherItem.querySelector('.industry-accordion-chevron');
              
              otherHeader.setAttribute('aria-expanded', 'false');
              otherContent.style.maxHeight = '0';
              otherChevron.style.transform = 'rotate(0deg)';
            }
          });
          
          // Toggle current accordion
          if (isExpanded) {
            header.setAttribute('aria-expanded', 'false');
            content.style.maxHeight = '0';
            chevron.style.transform = 'rotate(0deg)';
          } else {
            header.setAttribute('aria-expanded', 'true');
            content.style.maxHeight = content.scrollHeight + 'px';
            chevron.style.transform = 'rotate(180deg)';
          }
        });
      });
    });
  </script>

